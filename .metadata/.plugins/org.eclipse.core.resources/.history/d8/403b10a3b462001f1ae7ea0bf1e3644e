@RestController
public class DemoRestController {

	@GetMapping("/")
	public String doAction() {
		String message="Action in Progess...";
		try {
		int i=10/0;
		}
		catch(ArithmeticException a) {
			return "Exception Occured....";
		}
		finally{
			
		}
		return message;
	}
}
